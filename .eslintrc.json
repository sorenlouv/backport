{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": true
      }
    ],
    "no-underscore-dangle": "off",
    "no-use-before-define": ["error", { "functions": false }],
    "one-var": ["error", { "initialized": "never", "uninitialized": "always" }],
    "no-console": 0,
    "arrow-body-style": 0
  },
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "prettier"]
}
