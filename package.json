      "./test/setup.js"
    ]
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "chalk": "^2.3.0",
    "es6-promisify": "^6.0.0",
    "find-up": "^2.1.0",
    "inquirer": "^5.0.1",
    "joi": "^12.0.0",
    "lodash.get": "^4.4.2",
    "lodash.isempty": "^4.4.0",
