    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/jest": "^22.1.1",
    "babel-eslint": "^8.2.1",
    "eslint": "^4.16.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.1.4",
    "lint-staged": "^6.1.0",
    "nock": "^9.1.6",
    "prettier": "^1.10.2"
  }
